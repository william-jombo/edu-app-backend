# .htaccess - DO NOT set CORS headers here (handled by cors.php)

<IfModule mod_rewrite.c>
    RewriteEngine On
    
    # Handle OPTIONS preflight requests
    RewriteCond %{REQUEST_METHOD} OPTIONS
    RewriteRule ^(.*)$ $1 [R=200,L]
</IfModule>

# NOTE: CORS headers are handled in PHP (backend/includes/cors.php)
# to avoid duplicate headers
